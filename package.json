{
  "name": "seawhere",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:clean": "rm -rf .next && next dev",
    "build": "npm run lint && npm run lint:service && npm run lint:public-imports && npm run lint:admin-imports && npm run lint:public-batched && next build",
    "start": "next start",
    "lint": "next lint",
    "lint:service": "rg -n -e \"^\\s*import\\s+.*from\\s+['\\\"]@/lib/supabase/service['\\\"]\" -e \"require\\(['\\\"]@/lib/supabase/service['\\\"]\\)\" --glob '!node_modules' --glob '!eslint.config.mjs' && (echo '❌ service client found' && exit 1) || echo '✅ all clear'",
    "lint:public-imports": "rg --pcre2 -n \"from\\s+['\\\"]@/lib/data/(?!public/)\" src/app --glob '!src/app/admin/**' && (echo '❌ Import boundary violation in public pages' && exit 1) || echo '✅ public import boundaries ok'",
    "lint:admin-imports": "rg -n \"from\\s+['\\\"]@/lib/data/public/\" src/app/admin src/lib/data --glob '!src/lib/data/public/**' && (echo '❌ Admin/internal importing public helpers' && exit 1) || echo '✅ admin/public boundaries ok'",
    "lint:public-batched": "node scripts/check-public-batched.js"
  },
